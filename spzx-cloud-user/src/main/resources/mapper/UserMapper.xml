<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.awc20.spzx.cloud.user.mapper.UserMapper">
    <select id="selectAllUser" resultType="com.awc20.spzx.cloud.user.entity.User">
        select * from tb_user
    </select>
    <select id="selectUserByUserId" resultType="com.awc20.spzx.cloud.user.entity.User">
        select * from tb_user where id=#{userId}
    </select>
</mapper>